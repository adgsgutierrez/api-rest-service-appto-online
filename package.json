    "@types/node": "^17.0.42",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vercel/node": "^2.9.6",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0"
  }
}
